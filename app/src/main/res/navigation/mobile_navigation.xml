<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.coviddata.ui.home.AllCasesFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_cases_all" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.coviddata.ui.dashboard.CountriesCasesFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_cases_countries" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.coviddata.ui.notification.CountryCasesFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_cases_country" />
</navigation>